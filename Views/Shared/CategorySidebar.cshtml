                                            <li>@Html.ActionLink(item.ChildCategory.Name, "EditCategory", "Category", new { id = item.ChildCategory.Id }, null)</li>
                                        } 
                                    </ul>
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col2"></div>
                            <div class="col1"></div>
                            <div class="col1"></div>
                            <div class="col1"></div>
                            <div class="col1"></div>
                        </div>
                    </div>
                </li>
             
              

               
               
            </ul>
        </div>
    </div>
</div>



