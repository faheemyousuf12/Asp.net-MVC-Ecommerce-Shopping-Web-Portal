                                        } 
                                    </ul>
                                </div>
                            </div>
                            <div class="col1">
                                <div class="h_nav">
                                    <h4>Accessories</h4>
                                    <ul>
                                        @foreach (var item in Model)
                                        {
                                            <li>@Html.ActionLink(item.ChildCategory.Name, "EditCategory", "Category", new { id = item.ChildCategory.Id }, null)</li>
                                        } 
                                    </ul>
                                </div>
                            </div>
                            <div class="col1">
                                <div class="h_nav">
                                    <h4>Footwear</h4>
                                    <ul>
                                        @foreach (var item in Model)
