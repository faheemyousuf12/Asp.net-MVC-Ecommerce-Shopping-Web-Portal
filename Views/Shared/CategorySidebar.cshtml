@model IEnumerable<ECommerce.Models.Product>


<style>
    .animate {
        -webkit-transition: all .3s ease-in-out;
        -moz-transition: all .3s ease-in-out;
        -o-transition: all .3s ease-in-out;
        -ms-transition: all .3s ease-in-out;
        transition: all .3s ease-in-out;
    }

    .navbar-bootsnip {
        background-color: #2e353d;
        border-color: #5FB0E4;
        margin-bottom: 0;
    }
</style>
<div class="wrapper">
    <div class="container-fluid">
        <div class="col-md-12 navbar navbar-fixed-top navbar-bootsnip animate" role="navigation" style="z-index: 9999999">

            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class=" navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Help", "Index", "Home")</li>
                    <li>@Html.ActionLink("Delivery Information", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>

                </ul>

               @Html.Partial("_LoginPartial")



            </div>
        </div>
    </div>
</div>
<div class="header_bg">
    <div class="container">
        <div class="header">
            <div class="head-t">
                <div class="logo">
                    <a href="index.html"><img src="~/Images/logo.png" class="img-responsive" alt="" /> </a>
                </div>
                <!-- start header_right -->
                <div class="header_right">
                    <div class="rgt-bottom">
                        <div class="log">
                            <div class="login">
                                <div id="loginContainer">
                                    <a href="#" id="loginButton"><span>Login</span></a>
                                    <div id="loginBox">
                                        <form id="loginForm">
                                            <fieldset id="body">
                                                <fieldset>
                                                    <label for="email">Email Address</label>
                                                    <input type="text" name="email" id="email">
                                                </fieldset>
                                                <fieldset>
                                                    <label for="password">Password</label>
                                                    <input type="password" name="password" id="password">
                                                </fieldset>
                                                <input type="submit" id="login" value="Sign in">
                                                <label for="checkbox"><input type="checkbox" id="checkbox"> <i>Remember me</i></label>
                                            </fieldset>
                                            <span><a href="#">Forgot your password?</a></span>
                                        </form>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="reg">
                            <a href="register.html">REGISTER</a>
                        </div>
                        <div class="cart box_1">
                            <a href="checkout.html">
                                <h3> <span class="simpleCart_total">$0.00</span> (<span id="simpleCart_quantity" class="simpleCart_quantity">0</span> items)<img src="images/bag.png" alt=""></h3>
                            </a>
                            <p><a href="javascript:;" class="simpleCart_empty">(empty Card)</a></p>
                            <div class="clearfix"> </div>
                        </div>
                        <div class="create_btn">
                            <a href="checkout.html">
                            </a>
                        </div>
                        <div class="clearfix"> </div>
                    </div>
                    <div class="search col-md-12">
                        <form>
                            <input type="text" value="" placeholder="search...">
                            <input type="submit" value="">
                        </form>
                    </div>
                    <div class="clearfix"> </div>
                </div>
                <div class="clearfix"> </div>
            </div>
            <!-- start header menu -->
            <ul class="megamenu skyblue">
                <li class="active grid"><a class="color1" href="index.html">Home</a></li>
               
                <li class="grid">
                    <a class="color2" href="#">new arrivals</a>
                    <a class="color2" href="#">Mens</a>
                    <a class="color2" href="#">Womens</a>
                    <a class="color2" href="#">Babies</a>
                    <a class="color2" href="#">Watches</a>
                    <a class="color2" href="#">Footwear</a>
                    <div class="megapanel">
                        <div class="row">
                            <div class="col1">
                                <div class="h_nav">
                                  
                                 <h4>Clothing</h4>
                                    <ul>
                                       
                                        @foreach (var item in Model)
                                        {            
                                       <li>@Html.ActionLink(item.ChildCategory.Name, "EditCategory", "Category", new { id = item.ChildCategory.Id }, null)</li>
                                        } 
                                    </ul>
                                </div>
                            </div>
                            <div class="col1">
                                <div class="h_nav">
                                    <h4>kids</h4>
